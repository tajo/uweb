{
  "name": "uweb-web-code",
  "repository": "git@github.com:tajo/uweb.git",
  "author": "Vojtech Miksu <vojtech@miksu.cz>",
  "license": "MIT",
  "publisher": "voytuh",
  "displayName": "uweb",
  "description": "Make navigation in web-code easier.",
  "version": "1.0.3",
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "uweb.focusTerminal",
        "title": "uweb: Focus Terminal"
      },
      {
        "command": "uweb.focusExplorer",
        "title": "uweb: Focus Explorer"
      },
      {
        "command": "uweb.focusExplorerAndTerminal",
        "title": "uweb: Focus Explorer and Terminal"
      },
      {
        "command": "uweb.openSourcegraph",
        "title": "uweb: Open Sourcegraph for the current file"
      }
    ]
  },
  "activationEvents": [],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.67.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.16.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.2",
    "@vscode/test-electron": "^2.1.3"
  }
}
